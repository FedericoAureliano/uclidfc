module main {
    var i : integer;
    var d : integer;

    init {
        i = 0;
    }

    next {
        assume(d > 0);
        if (d > 0) {
            havoc i;
            assume(i > 0);
        } else {
            havoc i;
        }

        havoc d;
    }

    invariant cex: i == 0;

    control {
        induction;
        check;
        print_cex;
    }
}