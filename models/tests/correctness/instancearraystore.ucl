module fib
{
    var a, b : integer;

    init {
        a = 0;
        b = 1;
    }

    next {
        a' = b;
        b' = a + b;
    }
}

module main
{
    var fibs : [integer]fib;
    var fib1 : fib;

    type othertype;

    var a : integer;
    var b : boolean;
    var c : [integer][boolean]integer;

    var d : othertype;

    init {
        fibs[0] = fib1;
    }

    next {
        next(fibs[0]);
        next(fib1);
    }

    invariant musthold: fibs[0] == fib1;

    control
    {
        induction(2);
        check;
    }
}