// UCLID TEST
// Solver=z3
// Result=Some(false)

module fib {
  var a, b : integer;

  init {
    a = 0;
    b = 1;
  }

  next {
    a' = b;
    b' = a + b;
  }

  invariant a_le_b: a <= b;

    control
    {
        induction;
        check;
    }
}

type twofib = fib || fib;

module main {
    var double : twofib;

    next {
        next(double);
    }

    invariant notinductive: double.first.a <= double.first.b;
    invariant makesitinductive: double.first.a >= 0;

    control {
        induction;
        check;
    }
}