// UCLID TEST
// Solver=z3
// Rewrite=blast-enum-quantifiers
// Result=Some(true)

type cases = enum {A, B, C};

module main
{
    var arr: [cases]integer;

    invariant toblastexists: exists (x : cases) :: (forall (y : cases) :: arr[y] > 0) && arr[x] > 0;

    control
    {
        induction;
        check;
    }
}